{
  "manifest_version": 2,
  "name": "Hot Search",
  "description": "Quick search Chrome Extension with hot keys",
  "version": "1.1",
  "browser_action": {
     "default_icon": "icon.png",
     "default_popup": "popup.html"
  },
  "content_scripts":[{
      "matches":["<all_urls>"],
      "js":["fetch.js"]
  }] 
}
